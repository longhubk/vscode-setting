{
  "explorer.confirmDelete": false,
  "files.eol": "\r\n",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.vscode/**": true,
    "**/android/**": true,
    "**/ios/**": true
  },
  "search.exclude": {
    "**/node_modules/**": true,
    "**/bower_components": true,
    "**/*.code-search": true
  },
  "search.searchOnType": true,
  "search.sortOrder": "type",
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": false,
  "editor.mouseWheelZoom": false, // default
  "editor.showFoldingControls": "always",
  "editor.minimap.showSlider": "always",
  "editor.minimap.maxColumn": 90,
  "editor.minimap.size": "fill",
  "editor.smoothScrolling": false,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 90,
  "editor.rulers": [90],
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#FF2D00",
    "minimap.selectionHighlight": "#00FF00",
    "minimap.errorHighlight": "#FF0000",
    "minimap.warningHighlight": "#FFFF00",
    "minimap.background": "#1E1F27",
    "minimap.findMatchHighlight": "#f0f",
    "minimapSlider.background": "#D6C2C223",
    "editorLineNumber.activeForeground": "#CCCCCC",
    "tab.activeBackground": "#D6C2C223",
    "sideBar.border": "#3B3C40",
    "terminal.background": "#1E1F27",
    "terminal.foreground": "#DDDDDD",
    "terminalCursor.foreground": "#0f0",
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564",
    "editor.wordHighlightBorder": "#DDDD00",
    "editor.wordHighlightStrongBorder": "#00DD00"
  },
  "workbench.preferredDarkColorTheme": "Monokai Dark Vibrant",
  "workbench.colorTheme": "Monokai Dark Vibrant",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.titleScrollbarSizing": "large",
  "workbench.list.horizontalScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.enableExperiments": false,
  "window.titleBarStyle": "custom", //mother f*cker
  "window.newWindowDimensions": "inherit",
  "window.openFoldersInNewWindow": "on",
  "window.titleSeparator": " > ",
  "window.enableMenuBarMnemonics": false,
  "window.title": "${rootName}${separator}${activeEditorShort}" ,
  "terminal.integrated.cursorWidth": 3,
  "terminal.integrated.scrollback": 50000,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.confirmOnExit": "always",
  "terminal.integrated.confirmOnKill": "always",
  "terminal.integrated.tabs.title": "${workspaceFolder}",
  "terminal.integrated.tabs.showActiveTerminal": "always",
  "terminal.integrated.tabs.focusMode": "singleClick",
  "terminal.integrated.tabs.separator": " | ",
  "terminal.integrated.fontFamily": "DejaVu Sans Mono for Powerline",
  "terminal.integrated.defaultProfile.linux": "bash",
  "vim.handleKeys": { "<C-f>": false },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["t"],
      "after": ["5", "0"]
    },
    {
      "before": ["m"],
      "after": ["%"]
    },
  ],
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "cSpell.enabledLanguageIds": [
    "css",
    "html",
    "javascript",
    "markdown",
    "plaintext",
    "typescript"
  ],
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.showHiddenFiles": true,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "extensions.autoCheckUpdates": false,
  "linesinfostatusbar.alignment": "right",
  "linesinfostatusbar.totalDisplayFormat": "Lines: %d",
  "linesinfostatusbar.selectedDisplayFormat": " (%d)",
  "telemetry.telemetryLevel": "off",
  "json.maxItemsComputed": 100000,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.printWidth": 90,
  "security.workspace.trust.enabled": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.lineHeight": 1.3,
  "liveServer.settings.donotShowInfoMsg": true,
  "git.confirmSync": false,
  "debug.javascript.autoAttachFilter": "disabled",
  "git.suggestSmartCommit": false,
  "terminal.integrated.tabs.location": "left",
  "scm.providerCountBadge": "hidden",
  "workbench.editor.decorations.badges": false,
  "git.countBadge": "off",
  "scm.countBadge": "off",
  "outline.problems.badges": false,
  "breadcrumbs.icons": false,
  "outline.icons": false,
  "explorer.decorations.badges": false,
  "workbench.panel.defaultLocation": "right",
  "terminal.integrated.defaultLocation": "editor",
  "diffEditor.renderSideBySide": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "indentRainbow.colors": [
    "rgba(180,180,180,0.07)",
    "rgba(100,100,100,0.07)",
    "rgba(180,180,180,0.07)",
    "rgba(100,100,100,0.07)",
  ],
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "window.menuBarVisibility": "compact",
  "timeline.excludeSources": [
    "timeline.localHistory"
  ],
  "workbench.activityBar.visible": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.guides.bracketPairs": true,
  "editor.scrollbar.verticalScrollbarSize": 12,
  "editor.scrollbar.horizontalScrollbarSize": 8,
	"editor.selectionClipboard": false,

}
