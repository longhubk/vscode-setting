{
  "explorer.confirmDelete": false,
  "debug.allowBreakpointsEverywhere": true,
  "output.smartScroll.enabled": true,
  "files.associations": {
    "*.py": "python"
  },
  "files.eol": "\r\n",
  "files.autoSave": "off",
  "files.autoSaveDelay": 30000,
  "files.enableTrash": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  // "editor.fontFamily": "consolas",
  "editor.renderWhitespace": "all",
  "editor.accessibilityPageSize": 8,
  // "editor.fontSize": 18,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.mouseWheelZoom": true,
  "editor.showFoldingControls": "always",
  "editor.fontWeight": "100",
  "editor.lineHeight": 23,
  "editor.cursorWidth": 1,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.scale": 1,
  "editor.minimap.side": "right",
  "editor.minimap.maxColumn": 120,
  "editor.minimap.renderCharacters": true,
  "editor.smoothScrolling": false,
  "editor.stickyTabStops": false,
  "editor.wrappingStrategy": "advanced",
  "editor.tabCompletion": "onlySnippets",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "editor.rulers": [100],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/pq-mot/**": true
  },
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#FF2D00",
    "minimap.selectionHighlight": "#00FF00",
    "minimap.errorHighlight": "#FF0000",
    "minimap.warningHighlight": "#FFFF00",
    "minimap.background": "#1E1F27",
    "minimap.findMatchHighlight": "#f0f",
    "minimapSlider.background": "#D6C2C223",
    "editorLineNumber.activeForeground": "#CCCCCC",
    "tab.activeBackground": "#D6C2C223",
    "sideBar.border": "#3B3C40",
    "terminal.background": "#1E1F27",
    "terminal.foreground": "#DDDDDD",
    "terminalCursor.foreground": "#0f0",
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564",
    "editor.wordHighlightBorder": "#DDDD00",
    "editor.wordHighlightStrongBorder": "#00DD00"
  },
  "workbench.editor.labelFormat": "default",
  "workbench.editor.showIcons": true,
  "workbench.editor.tabSizing": "fit",
  "workbench.preferredDarkColorTheme": "Monokai Dark Vibrant",
  "workbench.colorTheme": "Monokai Dark Vibrant",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.titleScrollbarSizing": "large",
  "workbench.settings.editor": "ui",
  "workbench.list.horizontalScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "workbench.tips.enabled": true,
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": false,
  "workbench.editor.enablePreview": false,
  // "workbench.view.experimental.allowMovingToNewContainer": true,
  // "window.titleBarStyle": "native", //mother f*cker
  "window.titleBarStyle": "custom", //mother f*cker
  "window.closeWhenEmpty": false,
  "window.doubleClickIconToClose": false,
  "window.menuBarVisibility": "visible",
  "window.newWindowDimensions": "inherit",
  "window.openFoldersInNewWindow": "on",
  "window.title": "${dirty}${activeEditorShort}${separator}${activeEditorLong}${separator}${rootPath}",
  "terminal.integrated.cursorWidth": 3,
  // "terminal.integrated.fontSize": 17,
  "terminal.integrated.fontWeight": "100",
  "terminal.integrated.fontWeightBold": "100",
  "terminal.integrated.scrollback": 50000,
  // "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.lineHeight": 0,
  // "sync.gist": "54a75b3dd1eee51525ca3a74fc93097c",
  "search.searchOnType": true,
  "vim.handleKeys": { "<C-f>": false },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["t"],
      "after": ["5", "0"]
    },
    {
      "before": ["m"],
      "after": ["%"]
    }
  ],
  "update.mode": "none",
  "update.showReleaseNotes": false,
  // "powershell.powerShellDefaultVersion": "Windows PowerShell (x86)",
  "extensions.autoUpdate": false,
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "html",
    "java",
    "javascript",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "scss",
    "typescript"
  ],
  "cSpell.userWords": [
    "Giang",
    "Pressable",
    "Shoppay",
    "TWILIO",
    "bgcolor",
    "downasaur",
    "keras",
    "kien",
    "originalname",
    "renderselectedicon",
    "rfile",
    "wfile",
    "wifi"
  ],
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.showHiddenFiles": true,
  "javascript.validate.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "extensions.ignoreRecommendations": true,
  "gitlens.advanced.messages": {
    "suppressGitDisabledWarning": true,
    "suppressLineUncommittedWarning": true
  },
  "editor.accessibilitySupport": "off",
  "linesinfostatusbar.alignment": "right",
  "linesinfostatusbar.totalDisplayFormat": "Lines: %d",
  "linesinfostatusbar.selectedDisplayFormat": " (%d)",
  "typescript.disableAutomaticTypeAcquisition": true,
  "terminal.integrated.confirmOnExit": "always",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.vscode/**": true,
    "**/android/**": true,
    "**/ios/**": true
  },
  "workbench.enableExperiments": false,
  "extensions.autoCheckUpdates": false,
  "telemetry.telemetryLevel": "off",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "settingsSync.ignoredExtensions": [
  
  ],
  "json.maxItemsComputed": 100000,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.printWidth": 100,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "typescript.updateImportsOnFileMove.enabled": "always",
}
